{
    "name": "silverstripe/theme-colorpicker",
    "description": "Color picker module for the site available under admin settings",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.1",
        "silverstripe/colorpicker": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ThemeColorpicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilverStripe\\ThemeColorpicker\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
